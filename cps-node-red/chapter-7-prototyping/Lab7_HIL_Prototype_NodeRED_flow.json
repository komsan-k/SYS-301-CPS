  {
    "id": "fn_real",
    "type": "function",
    "z": "tab7",
    "name": "tag REAL",
    "func": "// Expect msg.payload like {value: 25} OR {t: 25}\nlet v = (msg.payload && (msg.payload.value ?? msg.payload.t)) ?? msg.payload;\nv = Number(v);\nmsg.topic = \"REAL\";\nmsg.payload = v;\nreturn msg;",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 560,
    "y": 200,
    "wires": [
      [
        "chart7",
        "dbg7"
      ]
    ]
  },
  {
    "id": "fn_ctrl",
    "type": "function",
    "z": "tab7",
    "name": "control: SIM > 30",
    "func": "// Control based on SIM series (incoming msg.payload numeric)\n// LED ON if value > 30 else OFF\nlet v = Number(msg.payload);\nlet cmd = (v > 30) ? \"ON\" : \"OFF\";\nmsg.topic = \"/cps/cmd/led\";\nmsg.payload = cmd;\nreturn msg;",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 610,
