  {
    "id": "fn_auto_control_lab4",
    "type": "function",
    "z": "tab_lab4",
    "name": "Auto control from prediction",
    "func": "// Auto-control policy example:\n// If prediction == 'DARK' => LED ON\n// If prediction == 'BRIGHT' => LED OFF\n// Only runs when Auto control is enabled.\n\nvar enabled = flow.get('lab4_auto_enable') || false;\nif (!enabled) return null;\n\nvar pred = ('' + msg.payload).trim().toUpperCase();\nvar cmd = null;\nif (pred === 'DARK') cmd = 'ON';\nif (pred === 'BRIGHT') cmd = 'OFF';\n\nif (cmd === null) return null;\nmsg.payload = cmd;\nmsg.topic = 'cps/lab4/led/set';\nreturn msg;",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 500,
    "y": 180,
    "wires": [
      [
        "mqttout_ledset_lab4",
        "debug_auto_cmd_lab4"
      ]
    ]
  },
