  {
    "id": "fn_manual_to_cmd_lab4",
    "type": "function",
    "z": "tab_lab4",
    "name": "Manual \u2192 ON/OFF",
    "func": "msg.payload = msg.payload ? 'ON' : 'OFF';\nmsg.topic = 'cps/lab4/led/set';\nreturn msg;",
